{
  "name": "team-creative-archive",
  "version": "1.0.0",
  "description": "Simple team creative archive with SQLite",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --ext js,html,css",
    "dev:watch": "concurrently \"npm run dev\" \"npm run watch:client\"",
    "watch:client": "nodemon --watch public --ext html,css,js --exec \"node scripts/reload-signal.js\"",
    "init-db": "node scripts/init-db.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "archive",
    "creative",
    "team",
    "sqlite"
  ],
  "author": "Your Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "ws": "^8.18.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
